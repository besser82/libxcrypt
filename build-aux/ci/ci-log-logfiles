#! /bin/sh

dump_log () {
    if [ -s "$1" ]; then
        echo "::group::$1"
        echo '::stop-commands::resume-50YEO1zJ8HSXH4Zy'
        cat "$1"
        echo '::resume-50YEO1zJ8HSXH4Zy::'
        echo '::endgroup::'
    fi
}

dump_log config.log
find . -name 'test-suite*.log' -printf '%P\n' | while read -r ts; do
    dump_log "$ts"
done
